.demos
  max-width 800px
.toggle-box
  a:hover
    text-decoration none
  h3,h2
    font-size 24px
    margin-bottom 16px
    font-weight bold
    line-height 40px
  h3
    font-size 20px
    line-height 36px
  h2
    cursor: pointer
    font-weight: normal
    span
      font-family consolas
      font-weight normal
      font-size 10px
ul.demos-list
  list-style none
  overflow: hidden;
  padding: 20px 20px;
  margin: 0 -20px;
  background unquote('url(data:image/png;base64,)')
  li
    list-style none
    display: inline;
    float: left;
    position: relative;
    margin-left: 10px;
    margin-right: 10px;
    background: white;
    background: rgba(255, 255, 255, 0.7);
    -moz-box-shadow: 0px 0px 5px 1px rgba(0, 0, 0, 0.1);
    -webkit-box-shadow: 0px 0px 5px 1px rgba(0, 0, 0, 0.1);
    box-shadow: 0px 0px 5px 1px rgba(0, 0, 0, 0.1);
    margin-bottom: 20px;
    width: 190px;
    height: 230px;
    padding 10px 15px;
    &:hover
      background: #efefef;
      background: rgba(0, 0, 0, 0.05);
      transition all 0.3s
  a
    overflow: hidden;
    height 100%
    max-width auto
    font-weight: bold;
    font-size: 20px;
    color: #0084b4;
    display block
    &:hover
      text-decoration none
  span
    font-size: 11px;
    margin-top: 8px;
    line-height 18px
    margin-bottom: 15px;
    color: #999;
    display block
  img
    display block

